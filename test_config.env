# iLand PostgreSQL Testing Configuration
# Copy this file to .env and modify as needed

# ============================================
# TESTING ENVIRONMENT SELECTION
# ============================================
# Set to 'local' for local PostgreSQL testing or 'production' for actual server
ENVIRONMENT=local

# ============================================
# LOCAL TESTING POSTGRESQL (for development)
# ============================================
# These settings are used when ENVIRONMENT=local
LOCAL_DB_NAME=iland_test_db
LOCAL_DB_USER=iland_test_user
LOCAL_DB_PASSWORD=iland_test_password
LOCAL_DB_HOST=localhost
LOCAL_DB_PORT=5433

# ============================================
# PRODUCTION POSTGRESQL (your actual server)
# ============================================
# These settings are used when ENVIRONMENT=production
# REPLACE WITH YOUR ACTUAL CREDENTIALS
PROD_DB_NAME=your_production_db_name
PROD_DB_USER=your_production_db_user
PROD_DB_PASSWORD=your_production_db_password
PROD_DB_HOST=your_production_db_host
PROD_DB_PORT=5432

# ============================================
# CURRENT ACTIVE CONFIGURATION
# ============================================
# These will be set dynamically based on ENVIRONMENT
DB_NAME=${LOCAL_DB_NAME}
DB_USER=${LOCAL_DB_USER}
DB_PASSWORD=${LOCAL_DB_PASSWORD}
DB_HOST=${LOCAL_DB_HOST}
DB_PORT=${LOCAL_DB_PORT}

# ============================================
# TABLE NAMES
# ============================================
SOURCE_TABLE=iland_md_data
DEST_TABLE=iland_embeddings

# ============================================
# BGE EMBEDDING CONFIGURATION
# ============================================
# BGE model for local processing (no API calls)
BGE_MODEL=bge-m3
BGE_CACHE_FOLDER=./cache/bge_models

# ============================================
# PROCESSING CONFIGURATION
# ============================================
CHUNK_SIZE=512
CHUNK_OVERLAP=50
BATCH_SIZE=20

# ============================================
# TEST DATA PATHS
# ============================================
TEST_CSV_FILE=../example/Chai_Nat/chai_nat_sample.csv
TEST_OUTPUT_DIR=./test_output
LOCAL_DATA_DIR=../data

# ============================================
# OPENAI CONFIGURATION (optional fallback)
# ============================================
# Only needed if you want to test OpenAI comparison
# OPENAI_API_KEY=your_openai_key_here

# ============================================
# LOGGING CONFIGURATION
# ============================================
LOG_LEVEL=INFO
LOG_FILE=./logs/test_pipeline.log 